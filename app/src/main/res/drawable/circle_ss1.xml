        <shape>
            <gradient android:angle="270" android:centerColor="#00CFFF" android:endColor="#00CFFF" android:startColor="#00CFFF" />

            <stroke android:width="0dp" android:color="#D1D2D9" />

            <corners android:radius="5dp" />
        </shape>
    </item>
