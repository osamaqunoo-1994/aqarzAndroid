<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="bottom">


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <FrameLayout
                    android:id="@+id/container"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_above="@+id/bottom_navigation"
                    app:layout_behavior="@string/appbar_scrolling_view_behavior"
                    tools:background="#EFF0F2" />


                <LinearLayout
                    android:id="@+id/gray_layout"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#90000000"
                    android:orientation="vertical"
                    android:visibility="gone" />

                <LinearLayout
                    android:id="@+id/service_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_centerInParent="true"
                    android:layout_marginBottom="100dp"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:visibility="gone">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:id="@+id/finance_c"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical">

                            <ImageView
                                android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:background="@drawable/circle_wh"
                                android:padding="12dp"
                                android:src="@drawable/ic_m_e3" />


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/m_e3"
                                android:textColor="@color/white" />


                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/Rental_installment_c"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical">

                            <ImageView
                                android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:background="@drawable/circle_wh"
                                android:padding="12dp"
                                android:src="@drawable/ic_m_e2" />


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/m_e2"
                                android:textColor="@color/white" />


                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/rate_c"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical">

                            <ImageView
                                android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:background="@drawable/circle_wh"
                                android:padding="12dp"
                                android:src="@drawable/ic_m_e1" />


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/m_e1"
                                android:textColor="@color/white" />


                        </LinearLayout>


                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_50sdp"
                        android:layout_marginTop="20dp"
                        android:layout_marginRight="@dimen/_50sdp"


                        android:orientation="horizontal">


                        <LinearLayout
                            android:id="@+id/add_aqarez_c"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical">

                            <ImageView
                                android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:background="@drawable/circle_wh"
                                android:padding="12dp"
                                android:src="@drawable/ic_m_e5" />


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/m_e4"
                                android:textColor="@color/white" />


                        </LinearLayout>


                        <LinearLayout
                            android:id="@+id/request_add_aqarez_c"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"

                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical">

                            <ImageView
                                android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:background="@drawable/circle_wh"
                                android:padding="12dp"
                                android:src="@drawable/ic_m_e4" />


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/m_e5"
                                android:textColor="@color/white" />


                        </LinearLayout>


                    </LinearLayout>


                    <TextView

                        android:id="@+id/add_aqar"
                        android:layout_width="200dp"
                        android:layout_height="42dp"
                        android:background="@drawable/back_click"
                        android:gravity="center"
                        android:text="@string/add_aqar"
                        android:textColor="#AAABB3"
                        android:textSize="15dp"
                        android:visibility="gone" />

                    <TextView
                        android:id="@+id/add_service_aqarez"

                        android:layout_width="200dp"
                        android:layout_height="42dp"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/back_click"
                        android:gravity="center"

                        android:text="@string/AqarsService"
                        android:textColor="#AAABB3"

                        android:textSize="15dp"
                        android:visibility="gone" />


                </LinearLayout>


                <LinearLayout
                    android:id="@+id/add_aqares_and_order_and_estate"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_marginLeft="20dp"

                    android:layout_marginRight="20dp"

                    android:layout_marginBottom="130dp"
                    android:background="@drawable/back_searcj"
                    android:orientation="vertical"
                    android:padding="10dp"
                    android:visibility="gone">

                    <ImageView
                        android:id="@+id/close_add"
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/ic_close_black_24dp" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:id="@+id/addAqares"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:padding="15dp">


                            <ImageView
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:background="@drawable/circle_fill"
                                android:padding="11dp"
                                android:src="@drawable/ic_plus" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp"
                                android:text="@string/add_aqar"
                                android:textColor="@color/black"
                                android:textSize="14dp"
                                android:textStyle="bold" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="10dp"
                            android:layout_height="wrap_content" />

                        <LinearLayout

                            android:id="@+id/RequstAqars"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:padding="15dp">


                            <ImageView
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:background="@drawable/circle_orange"
                                android:padding="10dp"
                                android:src="@drawable/ic_home11"
                                app:tint="@color/white" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp"
                                android:text="@string/AqarsService"
                                android:textColor="@color/black"
                                android:textSize="14dp"
                                android:textStyle="bold" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="10dp"
                            android:layout_height="wrap_content" />

                        <LinearLayout
                            android:id="@+id/rent"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:padding="15dp">


                            <ImageView
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:background="@drawable/circle_blue"
                                android:padding="10dp"
                                android:src="@drawable/ic_home_uuu"
                                app:tint="@color/white" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp"
                                android:text="@string/rent_aq1"
                                android:textColor="@color/black"
                                android:textSize="14dp"
                                android:textStyle="bold" />
                        </LinearLayout>

                    </LinearLayout>


                </LinearLayout>


            </RelativeLayout>


            <com.google.android.material.bottomappbar.BottomAppBar
                android:id="@+id/bottomBar"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_gravity="bottom"
                app:contentInsetEnd="0dp"
                app:contentInsetStart="0dp"

                app:fabAlignmentMode="center">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:visibility="visible">


                    <LinearLayout
                        android:layout_width="match_parent"

                        android:layout_height="match_parent"
                        android:layout_weight="1">


                        <LinearLayout
                            android:id="@+id/lay_1"

                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical">


                            <ImageView
                                android:id="@+id/image_1"
                                android:layout_width="22dp"
                                android:layout_height="22dp"
                                android:src="@drawable/ic_search_icon_ss"

                                />

                            <TextView
                                android:id="@+id/text_1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="2dp"
                                android:fontFamily="sans-serif-condensed-medium"
                                android:text="@string/search"
                                android:textSize="11dp" />


                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/lay_2"

                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical">


                            <ImageView
                                android:id="@+id/image_2"
                                android:layout_width="22dp"
                                android:layout_height="22dp"
                                android:src="@drawable/ic_favorite_bottom"

                                app:tint="#aaabb3" />

                            <TextView
                                android:id="@+id/text_2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="2dp"
                                android:fontFamily="sans-serif-condensed-medium"
                                android:text="@string/Favorites"
                                android:textColor="#898D97"
                                android:textSize="11dp" />


                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/lay_s"
                            android:layout_width="match_parent"

                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:visibility="invisible">


                            <ImageView
                                android:id="@+id/image_s"
                                android:layout_width="22dp"
                                android:layout_height="22dp"

                                android:src="@drawable/ic_not" />

                            <TextView
                                android:id="@+id/text_s"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="2dp"
                                android:fontFamily="sans-serif-condensed-medium"
                                android:text="@string/Notfication_bottm"
                                android:textColor="#898D97"
                                android:textSize="11dp" />


                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/lay_3"

                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical">


                            <ImageView
                                android:id="@+id/image_3"
                                android:layout_width="22dp"
                                android:layout_height="22dp"
                                android:src="@drawable/ic_chat_ss"
                                app:tint="#aaabb3" />

                            <TextView
                                android:id="@+id/text_3"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="2dp"
                                android:fontFamily="sans-serif-condensed-medium"
                                android:text="@string/chat"
                                android:textColor="#898D97"
                                android:textSize="11dp" />


                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/lay_4"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical">


                            <ImageView
                                android:id="@+id/image_4"
                                android:layout_width="22dp"
                                android:layout_height="22dp"
                                android:padding="2dp"

                                android:src="@drawable/ic_user_menu" />

                            <TextView
                                android:id="@+id/text_4"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="2dp"
                                android:fontFamily="sans-serif-condensed-medium"
                                android:text="@string/Profile_menu"
                                android:textColor="#898D97"
                                android:textSize="11dp" />


                        </LinearLayout>
                    </LinearLayout>


                </LinearLayout>


            </com.google.android.material.bottomappbar.BottomAppBar>

            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/fab"
                android:layout_width="64dp"
                android:layout_height="64dp"
                android:src="@drawable/ic_plus"
                android:tint="@color/white"
                android:visibility="visible"
                app:backgroundTint="@color/colorPrimary"

                app:fabCustomSize="64dp"
                app:layout_anchor="@id/bottomBar" />


        </androidx.coordinatorlayout.widget.CoordinatorLayout>
    </LinearLayout>

    <com.google.android.material.navigation.NavigationView
        android:id="@+id/navigationView_"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:fitsSystemWindows="true">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/close"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_margin="10dp"
                android:background="@drawable/circle_requst"
                android:paddingLeft="5dp"
                android:src="@drawable/ic_close_black_24dp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/din_mediam"
                android:gravity="center"
                android:text="@string/search_filtter_title"
                android:textSize="14dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="15dp"
                android:orientation="horizontal">


                <TextView
                    android:id="@+id/search"
                    android:layout_width="match_parent"
                    android:layout_height="38dp"
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:layout_weight="1"
                    android:background="@drawable/button_login"
                    android:gravity="center"
                    android:text="@string/search"
                    android:textColor="@color/white"


                    android:textSize="12dp" />

                <TextView
                    android:id="@+id/search_aqarez_man"
                    android:layout_width="match_parent"
                    android:layout_height="38dp"
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:layout_weight="1"
                    android:background="@drawable/mash"
                    android:gravity="center"

                    android:text="@string/search_aqarez_man"
                    android:textColor="#AAABB3"


                    android:textSize="12dp" />


            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:visibility="gone">


            </LinearLayout>

            <LinearLayout
                android:id="@+id/search_layout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginRight="10dp"
                    android:background="@drawable/bacccccback_select_city">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:padding="12dp"
                        android:src="@drawable/ic_loc" />

                    <EditText
                        android:id="@+id/search_text"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@android:color/transparent"
                        android:hint="@string/name_city_select"
                        android:imeOptions="actionSearch"
                        android:inputType="text"
                        android:paddingLeft="10dp"

                        android:paddingRight="10dp"
                        android:textColor="#000000"
                        android:textColorHint="#AAABB3"
                        android:textSize="13dp" />


                </LinearLayout>


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <LinearLayout
                        android:id="@+id/filtter_city"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/nodata"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="15dp"
                            android:gravity="center"
                            android:text="@string/nodata"
                            android:textSize="13dp"
                            android:visibility="gone" />

                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/allcity"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" />


                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/allfilter"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/opration"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_10sdp"
                            android:layout_marginTop="10dp"


                            android:visibility="visible" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"

                            android:visibility="visible">


                            <androidx.recyclerview.widget.RecyclerView
                                android:id="@+id/selsct_type_all"
                                android:layout_width="match_parent"

                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_10sdp"


                                android:visibility="visible" />


                        </LinearLayout>


                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dp"
                            android:layout_marginTop="20dp"
                            android:layout_marginRight="15dp"
                            android:fontFamily="@font/din_light"
                            android:text="@string/priceprice"
                            android:textColor="@color/colorPrimary"
                            android:textSize="15dp" />


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="20dp"
                            android:layout_marginTop="12dp"
                            android:layout_marginRight="20dp"
                            android:gravity="center"
                            android:paddingBottom="5dp">


                            <EditText
                                android:id="@+id/Les_price"
                                android:layout_width="match_parent"
                                android:layout_height="42dp"
                                android:layout_weight="1"
                                android:background="@drawable/mash"
                                android:fontFamily="@font/din_light"
                                android:gravity="center_vertical"
                                android:hint="@string/Les_price"
                                android:inputType="number"
                                android:paddingLeft="10dp"
                                android:paddingRight="10dp"
                                android:text=""
                                android:textColorHint="@color/hint_color"
                                android:textSize="@dimen/_10sdp" />

                            <LinearLayout
                                android:layout_width="10dp"
                                android:layout_height="wrap_content" />

                            <EditText
                                android:id="@+id/Maximum_price"
                                android:layout_width="match_parent"
                                android:layout_height="42dp"
                                android:layout_weight="1"
                                android:background="@drawable/mash"
                                android:fontFamily="@font/din_light"
                                android:gravity="center_vertical"
                                android:hint="@string/Maximum_price"
                                android:inputType="number"
                                android:paddingLeft="10dp"

                                android:paddingRight="10dp"
                                android:text=""
                                android:textColorHint="@color/hint_color"
                                android:textSize="@dimen/_10sdp" />


                        </LinearLayout>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dp"
                            android:layout_marginTop="20dp"
                            android:layout_marginRight="15dp"
                            android:fontFamily="@font/din_light"
                            android:text="@string/area_fi"
                            android:textColor="@color/colorPrimary"
                            android:textSize="15dp" />


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="20dp"
                            android:layout_marginTop="12dp"
                            android:layout_marginRight="20dp"
                            android:gravity="center"
                            android:paddingBottom="5dp">


                            <EditText
                                android:id="@+id/Les_space"
                                android:layout_width="match_parent"
                                android:layout_height="42dp"
                                android:layout_weight="1"
                                android:background="@drawable/mash"
                                android:fontFamily="@font/din_light"
                                android:gravity="center_vertical"
                                android:hint="@string/Les_space"
                                android:inputType="number"

                                android:paddingLeft="10dp"
                                android:paddingRight="10dp"
                                android:text=""
                                android:textColorHint="@color/hint_color"
                                android:textSize="@dimen/_10sdp" />

                            <LinearLayout
                                android:layout_width="10dp"
                                android:layout_height="wrap_content" />

                            <EditText
                                android:id="@+id/Maximum_space"
                                android:layout_width="match_parent"
                                android:layout_height="42dp"
                                android:layout_weight="1"
                                android:background="@drawable/mash"
                                android:fontFamily="@font/din_light"
                                android:gravity="center_vertical"
                                android:hint="@string/Maximum_space"
                                android:inputType="number"
                                android:paddingLeft="10dp"
                                android:paddingRight="10dp"

                                android:text=""
                                android:textColorHint="@color/hint_color"
                                android:textSize="@dimen/_10sdp" />


                        </LinearLayout>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dp"
                            android:layout_marginTop="20dp"
                            android:layout_marginRight="15dp"
                            android:fontFamily="@font/din_light"
                            android:text="@string/room"
                            android:textColor="@color/colorPrimary"
                            android:textSize="15dp" />


                        <HorizontalScrollView
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:layout_marginLeft="15dp"
                            android:layout_marginRight="15dp">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">


                                <TextView
                                    android:id="@+id/room_1"
                                    android:layout_width="45dp"
