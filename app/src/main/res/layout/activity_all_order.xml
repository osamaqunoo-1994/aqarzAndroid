<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F2F2F2">

    <LinearLayout
        android:layout_width="match_parent"

        android:layout_height="match_parent"
        android:orientation="vertical">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:background="@color/colorPrimary"
            android:gravity="center"
            android:orientation="horizontal">


            <ImageView
                android:id="@+id/back"

                android:layout_width="37dp"
                android:layout_height="37dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:background="@drawable/circle_fill"
                android:padding="8dp"
                android:src="@drawable/ic_back"
                app:tint="@color/white" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="55dp"

                android:layout_weight="1"
                android:fontFamily="@font/din_bold"
                android:gravity="center"
                android:text="@string/orders"

                android:textColor="@color/white"
                android:textSize="@dimen/_15sdp" />

            <ImageView
                android:layout_width="37dp"
                android:layout_height="37dp"
                android:layout_marginLeft="8dp"

                android:layout_marginRight="8dp"
                android:background="@drawable/circle_w"
                android:padding="8dp"
                android:src="@drawable/ic_filtter_city"
                android:visibility="invisible"
                app:tint="@color/colorPrimary" />

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"

            android:gravity="center"
            android:orientation="horizontal">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"


                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/Real_Estate_order_layout"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="5dp"
                    android:layout_weight="1"
                    android:background="@drawable/button_login"
                    android:gravity="center"
                    android:orientation="horizontal"

                    android:paddingLeft="5dp"
                    android:paddingRight="5dp">


                    <TextView
                        android:id="@+id/premium"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/circle_fill_g"
                        android:paddingLeft="5dp"
                        android:paddingRight="5dp"
                        android:text="@string/Enabled"

                        android:textColor="@color/white"
                        android:textSize="5dp" />

                    <ImageView
                        android:id="@+id/Real_Estate_order_image"

                        android:layout_width="17dp"
                        android:layout_height="17dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:src="@drawable/ic_group_1580" />


                    <TextView
                        android:id="@+id/Real_Estate_order_text"

                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="2dp"
                        android:gravity="center"

                        android:text="@string/Real_Estate_order"
                        android:textColor="@color/white"
                        android:textSize="11dp"
                        android:textStyle="bold" />

                    <ImageView
                        android:layout_width="10dp"
                        android:layout_height="20dp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/market_order_layout"
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="10dp"
                    android:visibility="visible"
                    android:layout_marginRight="5dp"
                    android:background="@drawable/circle_w"
                    android:gravity="center"
                    android:orientation="horizontal"

                    android:paddingLeft="5dp"
                    android:paddingRight="5dp">


                    <TextView
                        android:id="@+id/market_order_text"

                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginBottom="2dp"
                        android:gravity="center"

                        android:text="@string/title_market"
                        android:textColor="@color/colorPrimary"
                        android:textSize="11dp"
                        android:textStyle="bold" />


                </LinearLayout>


            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:visibility="gone">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="3dp"
                    android:layout_marginRight="3dp"

                    android:layout_weight="1">

                    <LinearLayout
                        android:id="@+id/Shopping_request_layout"
                        android:layout_width="wrap_content"
                        android:layout_height="38dp"
                        android:layout_marginTop="10dp"

                        android:weightSum="1">

                        <TextView
                            android:id="@+id/Shopping_request_text"
                            android:layout_width="wrap_content"

                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:paddingLeft="@dimen/_5sdp"
                            android:paddingRight="@dimen/_5sdp"

                            android:paddingBottom="3dp"
                            android:text="@string/Shoppingrequest"
                            android:textSize="12dp"
                            android:textStyle="bold"
                            android:visibility="gone" />
                    </LinearLayout>

                    <TextView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_alignParentEnd="true"
                        android:background="@drawable/circle_fill"
                        android:gravity="center"
                        android:paddingBottom="4dp"
                        android:text="+99"
                        android:textColor="@color/white"
                        android:textSize="11dp"
                        android:visibility="gone" />


                </RelativeLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"


                android:layout_marginTop="5dp"
                android:layout_marginRight="10dp"
                android:gravity="center">

                <ImageView
                    android:id="@+id/favorit_button"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:background="@drawable/circle_w"
                    android:padding="6dp"
                    android:src="@drawable/ic_love"
                    android:visibility="visible" />


            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="10dp"
            android:gravity="center"

            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/type_requst_xml"
                android:layout_width="match_parent"
                android:layout_height="41dp"
                android:layout_weight="1"
                android:background="@drawable/circle_wh"


                android:gravity="center"
                android:paddingLeft="3dp"
                android:paddingRight="3dp"
                android:visibility="visible">


                <RelativeLayout
                    android:layout_width="match_parent"

                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <TextView
                        android:id="@+id/AllOrder"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"

                        android:layout_marginTop="3dp"
                        android:layout_weight="1"
                        android:fontFamily="@font/din_light"
                        android:gravity="center"
                        android:paddingLeft="20dp"
                        android:paddingRight="20dp"
                        android:text="@string/all_order"
                        android:textColor="@color/black"
                        android:textSize="13dp" />

                    <TextView
                        android:id="@+id/AllOrder_number"

                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_alignParentEnd="true"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:background="@drawable/background_fill_red"
                        android:fontFamily="@font/din_bold"

                        android:gravity="center"
                        android:paddingBottom="3dp"
                        android:text="12"
                        android:textColor="@color/white"
                        android:textSize="8dp" />


                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <TextView
                        android:id="@+id/today"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"

                        android:layout_marginTop="3dp"
                        android:layout_weight="1"
                        android:background="@drawable/button_login"
                        android:fontFamily="@font/din_light"
                        android:gravity="center"
                        android:paddingLeft="20dp"
                        android:paddingRight="20dp"
                        android:text="@string/order_day"
                        android:textColor="@color/white"
                        android:textSize="13dp" />

                    <TextView
                        android:id="@+id/today_number"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_alignParentEnd="true"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:background="@drawable/background_fill_red"
                        android:fontFamily="@font/din_bold"
                        android:gravity="center"
                        android:paddingBottom="3dp"
                        android:text="12"
                        android:textColor="@color/white"
                        android:textSize="8dp" />


                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <TextView
                        android:id="@+id/Myoffer"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"

                        android:layout_marginTop="3dp"
                        android:layout_weight="1"
                        android:fontFamily="@font/din_light"
                        android:gravity="center"
                        android:paddingLeft="20dp"
                        android:paddingRight="20dp"
                        android:text="@string/MyOffer"
                        android:textColor="@color/black"
                        android:textSize="13dp" />

                    <TextView
                        android:id="@+id/Myoffer_number"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_alignParentEnd="true"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:background="@drawable/background_fill_red"
                        android:fontFamily="@font/din_bold"

                        android:gravity="center"
                        android:paddingBottom="3dp"
                        android:text="12"
                        android:textColor="@color/white"
                        android:textSize="8dp" />


                </RelativeLayout>

                <LinearLayout
                    android:id="@+id/show_fillter"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:gravity="center">


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/MoreFiltter"
                        android:textColor="@color/colorAccent"
                        android:visibility="gone" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_weight="1"
                        android:background="#20cccccc"
                        android:visibility="gone" />


                </LinearLayout>


            </LinearLayout>

            <ImageView
                android:id="@+id/open_filtter"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:background="@drawable/circle_fill"
                android:padding="8dp"

                android:src="@drawable/ic_filter"
                app:tint="@color/white" />
        </LinearLayout>

        <LinearLayout

            android:layout_width="match_parent"
            android:layout_height="41dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="10dp"
            android:background="@drawable/circle_wh"


            android:gravity="center"
            android:paddingLeft="3dp"
            android:paddingRight="3dp"
            android:visibility="visible">

            <TextView

                android:id="@+id/searcha_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"


                android:background="@drawable/button_login"
                android:fontFamily="@font/din_light"
                android:gravity="center"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:paddingBottom="5dp"
                android:text="@string/search"
                android:textColor="@color/white"
                android:textSize="13dp" />

            <LinearLayout
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="3dp"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="3dp"
                android:background="#D9D9D9" />

            <EditText
                android:id="@+id/search_text"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@android:color/transparent"
                android:hint="@string/search"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:textSize="11dp" />

            <LinearLayout
                android:id="@+id/all_status_offer"
                android:layout_width="wrap_content"
                android:orientation="horizontal"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/status_1"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"

                    android:layout_marginTop="3dp"

                    android:fontFamily="@font/din_light"
                    android:gravity="center"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:text="@string/asas1"
                    android:textColor="@color/black"
                    android:textSize="10dp" />

                <TextView
                    android:id="@+id/status_2"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"

                    android:layout_marginTop="3dp"
                    android:fontFamily="@font/din_light"
                    android:gravity="center"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:text="@string/asas2"
                    android:textColor="@color/black"
                    android:textSize="10dp" />

                <TextView
                    android:id="@+id/status_3"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"

                    android:layout_marginTop="3dp"
                    android:fontFamily="@font/din_light"
                    android:gravity="center"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:text="@string/asas3"
                    android:textColor="@color/black"
                    android:textSize="10dp" />

            </LinearLayout>



        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.core.widget.NestedScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/not_premium"

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:visibility="gone">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:paddingLeft="2dp"
                            android:paddingRight="2dp"
                            android:text="@string/requered_found"
                            android:textColor="#ee5c5c"
                            android:textSize="11dp" />

                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:padding="5dp"
                            android:src="@drawable/ic_not_prumem" />
                    </LinearLayout>


                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/AllResultRec"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">


                    </androidx.recyclerview.widget.RecyclerView>


                    <ProgressBar
                        android:id="@+id/progress"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_centerInParent="true"
                        android:visibility="gone" />


                    <LinearLayout
                        android:id="@+id/nodata_vis"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:visibility="gone">

                        <ImageView
                            android:layout_width="150dp"
                            android:layout_height="150dp"
                            android:layout_marginTop="20dp"
                            android:alpha=".3"
                            android:src="@drawable/ic_no_data" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="15dp"
                            android:text="@string/nodata"
                            android:textColor="@color/textColor2" />


                    </LinearLayout>


                </LinearLayout>
            </androidx.core.widget.NestedScrollView>


        </RelativeLayout>


    </LinearLayout>


    <com.google.android.material.navigation.NavigationView
        android:id="@+id/navigationView_"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:fitsSystemWindows="true">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/close"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_margin="10dp"
                android:background="@drawable/circle_requst"
                android:paddingLeft="5dp"
                android:src="@drawable/ic_close_black_24dp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/din_mediam"
                android:gravity="center"
                android:text="@string/search_filtter_title"
                android:textSize="14dp" />


            <LinearLayout
                android:id="@+id/search_layout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="10dp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginRight="10dp"
                    android:background="@drawable/bacccccback_select_city">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:padding="12dp"
                        android:src="@drawable/ic_loc" />

                    <EditText
                        android:id="@+id/search_citytext"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@android:color/transparent"
                        android:hint="@string/name_city_select"
                        android:imeOptions="actionSearch"
                        android:inputType="text"
                        android:paddingLeft="10dp"

                        android:paddingRight="10dp"
                        android:textColor="#000000"
                        android:textColorHint="#AAABB3"
                        android:textSize="13dp" />


                </LinearLayout>


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <LinearLayout
                        android:id="@+id/filtter_city"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:visibility="gone">

                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/allcity"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" />


                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/allfilter"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dp"
                            android:layout_marginTop="20dp"
                            android:layout_marginRight="15dp"
                            android:fontFamily="@font/din_light"
                            android:text="@string/t1_details"
                            android:textColor="@color/colorPrimary"
                            android:textSize="15dp" />

                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/all_type"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_10sdp"
                            android:layout_marginTop="10dp"


                            android:visibility="visible" />


                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dp"
                            android:layout_marginTop="20dp"
                            android:layout_marginRight="15dp"
                            android:fontFamily="@font/din_light"
                            android:text="@string/priceprice"
                            android:textColor="@color/colorPrimary"
                            android:textSize="15dp" />


                        <LinearLayout
                            android:id="@+id/price_fund"

                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="20dp"
                            android:layout_marginTop="12dp"
                            android:layout_marginRight="20dp"
                            android:gravity="center"
                            android:paddingBottom="5dp">


                            <TextView
                                android:id="@+id/select_price"
                                android:layout_width="match_parent"
                                android:layout_height="42dp"
                                android:layout_weight="1"
                                android:background="@drawable/mash"
                                android:drawableEnd="@drawable/ic_baseline_arrow_drop_down_24"
                                android:fontFamily="@font/din_light"
                                android:gravity="center_vertical"
                                android:hint="@string/priceprice"
                                android:paddingLeft="10dp"
                                android:paddingRight="10dp"
                                android:text=""
                                android:textColorHint="@color/hint_color"
                                android:textSize="@dimen/_10sdp" />


                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/price_market"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="20dp"
                            android:layout_marginTop="12dp"
                            android:layout_marginRight="20dp"
                            android:gravity="center"
                            android:paddingBottom="5dp">


                            <EditText
                                android:id="@+id/Les_price"
                                android:layout_width="match_parent"
                                android:layout_height="42dp"
                                android:layout_weight="1"
                                android:background="@drawable/mash"
                                android:fontFamily="@font/din_light"
                                android:gravity="center_vertical"
                                android:hint="@string/Les_price"
                                android:inputType="number"
                                android:paddingLeft="10dp"
                                android:paddingRight="10dp"
                                android:text=""
                                android:textColorHint="@color/hint_color"
                                android:textSize="@dimen/_10sdp" />

                            <LinearLayout
                                android:layout_width="10dp"
                                android:layout_height="wrap_content" />

                            <EditText
                                android:id="@+id/Maximum_price"
                                android:layout_width="match_parent"
                                android:layout_height="42dp"
                                android:layout_weight="1"
                                android:background="@drawable/mash"
                                android:fontFamily="@font/din_light"
                                android:gravity="center_vertical"
                                android:hint="@string/Maximum_price"
                                android:inputType="number"
                                android:paddingLeft="10dp"

                                android:paddingRight="10dp"
                                android:text=""
                                android:textColorHint="@color/hint_color"
                                android:textSize="@dimen/_10sdp" />


                        </LinearLayout>


                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dp"
                            android:layout_marginTop="20dp"
                            android:layout_marginRight="15dp"
                            android:fontFamily="@font/din_light"
                            android:text="@string/area_fi"
                            android:textColor="@color/colorPrimary"
                            android:textSize="15dp" />

                        <LinearLayout
                            android:id="@+id/area_market"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="20dp"
                            android:layout_marginTop="12dp"
                            android:layout_marginRight="20dp"
                            android:gravity="center"
                            android:paddingBottom="5dp">


                            <EditText
                                android:id="@+id/Les_space"
                                android:layout_width="match_parent"
                                android:layout_height="42dp"
                                android:layout_weight="1"
                                android:background="@drawable/mash"
                                android:fontFamily="@font/din_light"
                                android:gravity="center_vertical"
                                android:hint="@string/Les_space"
                                android:inputType="number"

                                android:paddingLeft="10dp"
                                android:paddingRight="10dp"
                                android:text=""
                                android:textColorHint="@color/hint_color"
                                android:textSize="@dimen/_10sdp" />

                            <LinearLayout
                                android:layout_width="10dp"
                                android:layout_height="wrap_content" />

                            <EditText
                                android:id="@+id/Maximum_space"
                                android:layout_width="match_parent"
                                android:layout_height="42dp"
                                android:layout_weight="1"
                                android:background="@drawable/mash"
                                android:fontFamily="@font/din_light"
